<Window x:Class="WpfApp1.WinCheck"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Register as a runner" Height="550" Width="800" FontFamily="Arial">
    <Canvas>
        <Rectangle Height="55" Fill="#FF424242" Width="800"/>
        <Label Foreground="White" FontSize="30" Content="Marathon Skills 2015" Canvas.Left="109" Canvas.Top="5"/>

        <Rectangle Height="44" Fill="#FF424242" Width="800" Canvas.Left="-1" Canvas.Top="490"/>
        <Label Content="" Foreground="White" Name="lblTimer" Canvas.Left="256" Canvas.Top="495"/>
        <Button Name="back" Click="back_Click" Content="Back" Canvas.Left="24" Canvas.Top="16" Height="24" Width="65"/>
        <Button Content="I have cometed before" Name="login" Click="login_Click" Canvas.Left="291" Canvas.Top="130" Height="52" Width="218"/>
        <Button Click="Button_Click" Content="I am a new competitor" Canvas.Left="291" Canvas.Top="200" Height="52" Width="218"/>
        <Button Content="Login" x:Name="login_Copy" Click="login_Click" Canvas.Left="630" Canvas.Top="439" Height="32" Width="93"/>


    </Canvas>
</Window>
